  "description": "Tools-page",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=development webpack --progress --debug",
    "build:prod:stage": "cross-env NODE_ENV=production STAGE=stage webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --debug",
    "prod": "cross-env NODE_ENV=production webpack && ./node_modules/.bin/http-server build -p 4200",
    "serve": "./node_modules/.bin/http-server build -p 4200",
    "serve:root": "./node_modules/.bin/http-server build/tools -p 4200",
    "e2e:local": "./node_modules/.bin/protractor protractor.conf.js",
    "e2e:local:mobile": "cross-env DEVICE=mobile ./node_modules/.bin/protractor protractor.conf.js",
    "e2e:local:tablet": "cross-env DEVICE=tablet ./node_modules/.bin/protractor protractor.conf.js",
    "postinstall": "./node_modules/protractor/bin/webdriver-manager update",
    "ncu-vizabi": "ncu -u vizabi vizabi vizabi-barchart-ds vizabi-barrankchart vizabi-bubblechart vizabi-bubblemap vizabi-config-systema_globalis vizabi-ddfcsv-reader vizabi-extapimap vizabi-linechart vizabi-mountainchart vizabi-popbyage vizabi-sankey vizabi-spreadsheet vizabi-ws-reader"
  },
