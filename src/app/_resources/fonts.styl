@import 'fonts.ru.styl'

$fonts_dir := "/assets/fonts/";

/* source-sans-pro-200 - latin-ext_latin */
@font-face {
  font-family: 'Source Sans Pro Thin';
  font-style: normal;
  font-weight: 200;
  src: url($fonts_dir + 'source-sans-pro-v11-latin-ext_latin-200.eot'); /* IE9 Compat Modes */
  src: local('Source Sans Pro ExtraLight'), local('SourceSansPro-ExtraLight'),
       url($fonts_dir + 'source-sans-pro-v11-latin-ext_latin-200.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url($fonts_dir + 'source-sans-pro-v11-latin-ext_latin-200.woff2') format('woff2'), /* Super Modern Browsers */
       url($fonts_dir + 'source-sans-pro-v11-latin-ext_latin-200.woff') format('woff'), /* Modern Browsers */
       url($fonts_dir + 'source-sans-pro-v11-latin-ext_latin-200.ttf') format('truetype'), /* Safari, Android, iOS */
       url($fonts_dir + 'source-sans-pro-v11-latin-ext_latin-200.svg#SourceSansPro') format('svg'); /* Legacy iOS */
}
